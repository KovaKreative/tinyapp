  <%- include('partials/_header', { title: "TinyApp" }) %>
    <main style="margin: 1em;">
      <div class="container">
        <h3>My URLs</h3>
        <table class="table">
          <thead>
            <tr>
              <th scope="col">Short URL ID</th>
              <th scope="col">Long URL</th>
              <th scope="col">Edit</th>
              <th scope="col">Delete</th>
            </tr>
          </thead>
          <tbody>
            <% for (let id in urls) { %>
              <tr>
                <td>
                  <%= id %>
                </td>
                <td>
                  <%= urls[id] %>
                </td>
                <td>
                  <form method="GET" action="/urls/<%= id %>"><button type="submit"
                      class="btn btn-outline-primary">Edit</button></form>
                </td>
                <td>
                  <form method="POST" action="/urls/delete/<%= id %>"><button type="submit"
                      class="btn btn-outline-danger">Delete</button></form>
                </td>
              </tr>
              <% } %>
          </tbody>
        </table>
      </div>
    </main>
    <%- include('partials/_footer') %>